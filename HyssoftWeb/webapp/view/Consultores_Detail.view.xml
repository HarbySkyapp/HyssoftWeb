<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="HyssoftWeb.HyssoftWeb.controller.Consultores_Detail" displayBlock="true">
	<Page xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" id="detail" title="{i18n>DetailTitle}" showNavButton="true" navButtonPress="action" action:wiring="\{'navButtonPress':\{'navigation':\{'routeName':'Consultores_Lista'\}\}\}">
		<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" editable="true" layout="ResponsiveGridLayout" id="form0">
			 <sap.ui.layout.form:content>
		        <sap.ui.core:Title text="Editar Consultor" id="title1"/>
		        	<Label text="NOMBRE" id="NAME"/>
					<Input width="100%" value="{NOMBRE}" id="NOMBRE"/>
					<Label text="CEDULA" id="IDENTIFICATION"/>
					<Input width="100%" id="CEDULA" value="{CEDULA}"/>
					<Label text="CARGO" id="POSITION"/>
					<Input width="100%" id="CARGO" value="{CARGO}"/>
				     <Label text="CIUDAD" id="CITY"/>
				     <Input width="100%" id="CIUDAD" value="{CIUDAD}"/>
				     <Label text="PAIS" id="COUNTRY"/>
				     <Input width="100%" id="PAIS" value="{PAIS}"/>
				     <Label text="TELEFONO" id="PHONE"/>
				     <Input width="100%" id="TELEFONO" value="{TELEFONO}"/>
				     <Label text="CELULAR" id="CELLPHONE"/>
				     <Input width="100%" id="CELULAR" value="{CELULAR}"/>
				     
		    </sap.ui.layout.form:content>
		</sap.ui.layout.form:SimpleForm>
		<FlexBox width="100%" id="box0" justifyContent="Center">
		    <items>
		        <Button xmlns="sap.m" text="Modificar" id="button0" press="modificarconsultor" width="150px"/>
		        <Button xmlns="sap.m" text="Eliminar" id="button1" width="150px" press="onDelete0"/>
		    </items>
		</FlexBox>
	<content/>
	</Page>
</mvc:View>