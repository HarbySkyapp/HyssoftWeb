<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="HyssoftWeb.HyssoftWeb.controller.Vpn_Crear">
	<App>
		<pages>
			<Page title="Agregar Vpn">
				<content>
						<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" layout="ResponsiveGridLayout" id="form0">
				        <sap.ui.layout.form:content>
				            <core:Title text="Title" id="title0"/>
				            
				            <Label text="ID" id="id"/>
				            <Input width="100%" id="ID"/>
				            
				            <Label text="VPN" id="vpn"/>
				            <Input width="100%" id="VPN"/>
				            
				            <Label text="USUARIO" id="usuario"/>
				            <Input width="100%" id="USUARIO"/>
				            
				            <Label text="CLAVE" id="clave"/>
				            <Input width="100%" id="CLAVE"/>
				            
				            <Label text="INFORMACION" id="informacion"/>
				            <Input width="100%" id="INFORMACION"/>
				            
				            <Label text="NIT EMP" id="nit_emp"/>
				            <Input width="100%" id="NIT_EMP"/>
				            
				            <Button xmlns="sap.m" text="Almacenar" id="button0" press="crearVpn"/>
				            <Button xmlns="sap.m" text="Cancelar" id="button1"/>
				         </sap.ui.layout.form:content>
				    </sap.ui.layout.form:SimpleForm>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>