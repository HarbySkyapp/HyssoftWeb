<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="HyssoftWeb.HyssoftWeb.controller.Ambientesap_detail" xmlns:html="http://www.w3.org/1999/xhtml">
		<Page xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" id="detail" title="{i18n>DetailTitle}" showNavButton="true" navButtonPress="action" action:wiring="\{'navButtonPress':\{'navigation':\{'routeName':'Ambientes_Lista'\}\}\}">
		<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" editable="true" layout="ResponsiveGridLayout" id="form0">
			 <sap.ui.layout.form:content>
		        <sap.ui.core:Title text="Editar Ambiente Sap " id="title1"/>
		        	<Label text="ID" id="id"/>
					<Input width="60%" value="{ID}" id="ID"/>
					<Label text="Ambiente Sap" id="Ambientesap"/>
					<Input width="60%" id="AMBSAP" value="{AMBSAP}"/>
					<Label text="RUTA" id="ruta"/>
					<Input width="60%" id="RUTA" value="{RUTA}"/>
				     <Label text="Empresa" id="nit"/>
				     <Input width="60%" id="NIT_NIT" value= "{NIT_NIT}"/>
		    </sap.ui.layout.form:content>
		</sap.ui.layout.form:SimpleForm>
		<FlexBox width="100%" id="box0" justifyContent="Center">
		    <items>
		        <Button xmlns="sap.m" text="Modificar" id="button0" press="modificarAmbiente" width="150px"/>
		        <Button xmlns="sap.m" text="Eliminar" id="button1" width="150px" press="onDelete0"/>
		    </items>
		</FlexBox>
	<content/>
	</Page>
</mvc:View>