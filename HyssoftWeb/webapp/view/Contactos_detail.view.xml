<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="HyssoftWeb.HyssoftWeb.controller.Contactos_detail">
<Page xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" id="detail" title="{i18n>DetailTitle}" showNavButton="true" navButtonPress="action" action:wiring="\{'navButtonPress':\{'navigation':\{'routeName':'Contactos_Lista'\}\}\}">
		<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" editable="true" layout="ResponsiveGridLayout" id="form0">
			 <sap.ui.layout.form:content>
		        <sap.ui.core:Title text="Editar Consultor" id="title1"/>
		        	<Label text="NOMBRE" id="nombre"/>
					<Input width="100%" value="{NOMBRE}" id="NOMBRE"/>
					<Label text="CARGO" id="cargo"/>
					<Input width="100%" id="CARGO" value="{CARGO}"/>
					<Label text="EMAIL" id="email"/>
					<Input width="100%" id="EMAIL" value="{EMAIL}"/>
				     <Label text="TELEFONO 1" id="teluno"/>
				     <Input width="100%" id="TELUNO" value="{TELUNO}"/>
				     <Label text="TELEFONO 2" id="teldos"/>
				     <Input width="100%" id="TELDOS" value="{TELDOS}"/>
				     <Label text="EMPRESA" id="NIT"/>
				     <Input width="100%" id="NIT_NIT" value="{NIT_NIT}"/>
		    </sap.ui.layout.form:content>
		</sap.ui.layout.form:SimpleForm>
		<FlexBox width="100%" id="box0" justifyContent="Center">
		    <items>
		        <Button xmlns="sap.m" text="Modificar" id="button0" press="modificarcontacto" width="150px"/>
		        <Button xmlns="sap.m" text="Eliminar" id="button1" width="150px" press="onDelete0"/>
		    </items>
		</FlexBox>
	<content/>
	</Page>
</mvc:View>