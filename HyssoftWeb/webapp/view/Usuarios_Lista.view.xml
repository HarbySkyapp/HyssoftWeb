<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="HyssoftWeb.HyssoftWeb.controller.Usuarios_Lista">
	<App>
		<pages>
			<Page xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" title="Usuarios" showNavButton="true" navButtonPress="action" action:wiring="\{'navButtonPress':\{'navigation':\{'routeName':'menu1'\}\}\}">
				<content>
					<Panel>
						<headerToolbar>
							<Toolbar>
								<Title/>
								<ToolbarSpacer/>
								<SearchField width="50%" search=".onFilterProducts" placeholder="Buscar Usuario"/>
							</Toolbar>
						</headerToolbar>
						<Panel>
					    	<headerToolbar>
		                        <OverflowToolbar>
		                            <ToolbarSpacer/>
		                            <OverflowToolbarButton xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" type="Transparent" icon="sap-icon://add" press="action" action:wiring="\{'press':\{'navigation':\{'routeName':'Usuarios_crear'\}\}\}"/>
		                        </OverflowToolbar>
		                    </headerToolbar>
						</Panel>
					    <List xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" noDataText="Drop list items here" items="{ path: '/Usuarios'}" id="Usuarios" cd:w5g.dt.context="/Empresas">
					        <items>
					            <StandardListItem type="Navigation" icon="sap-icon://building" id="item1" title="{ID}" description="{NIT_NIT}" press="handleListItemPress"/>
					        </items>
					    </List>
					</Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>